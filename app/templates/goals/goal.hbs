<div class="goal-display">
	<img src="http://placehold.it/350x150&text=D3chart"><br/>
	<div class="dates">
		<p class="left">Created: {{format-date createdAt}}</p>
		<p class="right">Last Edited: {{format-date lastEdited}}</p>
	</div>
</div>

<h2>{{title}}</h2>

<p class="description">{{description}}</p> 
<ul> 
	{{#each steps}}
		{{partial 'step'}}
	{{/each}}
</ul>

{{outlet}}
